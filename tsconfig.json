{
  "compilerOptions": {
    "noImplicitAny": false,
    "target": "ES5",
    "downlevelIteration": true,
    "module": "ESNext",
    "moduleResolution": "node",
    "lib": ["DOM", "ESNext", "es6"],
    "jsx": "react-jsx",
    "noEmit": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
  },
  "include": ["src/**/*"]
}
